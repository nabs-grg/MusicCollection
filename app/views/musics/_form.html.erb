<%= simple_form_for @music do |f| %>
	<%= f.input :album, lablel: "Album Title"%>
	<%= f.input :track%>
	<%= f.input :artist%>
	<%= f.input :released_year%>
	<%= select_tag(:genre_id, options_for_select(@genre), :prompt => "Select a genre") %>
	<%= f.file_field :music_album_img %> 
	<%= f.button :submit%>
<% end %>